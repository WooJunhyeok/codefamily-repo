{
  "family": "myapp-taskdef",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::969702370947:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::969702370947:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "myapp-container",
      "image": "{{IMAGE_URI}}",
      "portMappings": [{ "containerPort": 80 }]
    }
  ]
}
